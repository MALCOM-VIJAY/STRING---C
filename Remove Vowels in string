//to print
#include <stdio.h>
int main()
{
    int flag=0,i=0;
    char a[]={"malcom vijay josephraj"};
    while(a[i]!='\0'){
        if(a[i]=='a'||a[i]=='e'||a[i]=='i'||a[i]=='o'||a[i]=='u'){
            a[i]=1;
        }
        i++;
    }
    printf("%s",a);
    return 0;
}
