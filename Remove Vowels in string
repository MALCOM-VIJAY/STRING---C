#include <stdio.h>
#include <string.h>
int main()
{
    char a[]={"this is janagan"};
    int n=strlen(a);
    int j=0,i;
    for(i=0;i<=n;i++){
        if(a[i]!='a'&& a[i]!='e'&& a[i]!='i'&& a[i]!='o'&& a[i]!='u'){
            a[j]=a[i];
            j++;
        }
    }
    printf("%s",a);
    return 0;
}


